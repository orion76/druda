{#
/**
 * @file entity_link.html.twig
 * Default theme implementation to present Entity link data.
 *
 * This template is used when viewing Entity link pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_entity_link()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('entity_link') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
